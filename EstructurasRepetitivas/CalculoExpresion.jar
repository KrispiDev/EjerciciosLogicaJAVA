import java.util.Scanner;

public class CalculoExpresion {

    public static void main(String[] args) {
        // Crear un objeto Scanner para leer la entrada del usuario
        Scanner scanner = new Scanner(System.in);

        // Solicitar al usuario el valor n
        System.out.print("Ingrese el valor de n: ");
        int n = scanner.nextInt();

        // Calcular el valor de la expresión
        double resultado = calcularExpresion(n);

        // Mostrar el resultado
        System.out.println("El resultado de la expresión para n=" + n + " es: " + resultado);
    }

    // Función para calcular el valor de la expresión
    static double calcularExpresion(int n) {
        double suma = 0.0;

        // Calcular la sumatoria
        for (int i = 1; i <= n; i++) {
            double producto = 1.0;

            // Calcular el producto para cada valor de j
            for (int j = n; j >= i; j--) {
                producto *= j;
            }

            // Sumar el término actual a la sumatoria
            suma += (1.0 / i) + producto;
        }

        return suma;
    }
}
